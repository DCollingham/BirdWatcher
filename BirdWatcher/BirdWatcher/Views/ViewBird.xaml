<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BirdWatcher.Views.ViewBird">
    <ContentPage.Content>
        <StackLayout>
            <Image x:Name="ImageName"/>
            <Label x:Name="birdName"/>
            <Label x:Name="birdLocation"/>
            <Label x:Name="birdFamily"/>
            <Label x:Name="birdSpecies"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>